{
  "name": "kippy.online",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:tmp": "rimraf tmp",
    "clean": "run-s clean:*",
    "serve:webpack": "webpack --mode development --watch",
    "serve:postcss": "postcss src/styles/main.css --o tmp/main.css --watch",
    "serve:eleventy": "ELEVENTY_ENV=development eleventy --serve",
    "serve": "npm-run-all clean:* --parallel serve:*",
    "build:webpack": "webpack --mode production",
    "build:postcss": "NODE_ENV=production postcss src/styles/main.css --o tmp/main.css",
    "build:eleventy": "ELEVENTY_ENV=production eleventy",
    "build": "run-s clean:* build:*"
  },
  "dependencies": {
    "@hotwired/turbo": "^7.0.0-beta.4",
    "autoprefixer": "^10.2.4",
    "postcss": "^8.2.5",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "prismjs": "^1.23.0",
    "tailwindcss": "^2.0.3",
    "vivere": "^0.0.38"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "cssnano": "^4.1.10",
    "fs": "^0.0.1-security",
    "markdown-it-anchor": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  }
}
